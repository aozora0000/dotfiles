#################################
# 環境設定ファイル呼び出し
#################################
source $HOME/.dotfiles/.env
source $HOME/.dotfiles/.path

#################################
# エイリアスファイル呼び出し
#################################
source $HOME/.dotfiles/.alias

#################################
# ファンクション呼び出し
#################################
source $HOME/.dotfiles/.functions

#################################
# .functions.d 呼び出し
#################################
read-dir $HOME/.dotfiles/.functions.d/

#################################
# .brew.d 呼び出し
#################################
read-dir $HOME/.dotfiles/.brew.d/

#################################
# Prezto 呼び出し
#################################
source $HOME/.dotfiles/prezto.zsh

#################################
# Prezto 呼び出し
#################################
read-dir $HOME/.dotfiles/.zplugin.d/
