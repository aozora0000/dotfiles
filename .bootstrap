#################################
# 環境設定ファイル呼び出し
#################################
source $HOME/.profile/.env
source $HOME/.profile/.path

#################################
# エイリアスファイル呼び出し
#################################
source $HOME/.profile/.alias

#################################
# ファンクション呼び出し
#################################
source $HOME/.profile/.functions

#################################
# .functions.d 呼び出し
#################################
read-dir $HOME/.profile/.functions.d/


#################################
# .zsh.d 呼び出し
#################################
read-dir $HOME/.profile/.zsh.d/

#################################
# .brew.d 呼び出し
#################################
read-dir $HOME/.profile/.brew.d/
