#################################
# 環境設定ファイル呼び出し
#################################
source $HOME/.profile/.env
source $HOME/.profile/.path

#################################
# エイリアスファイル呼び出し
#################################
source $HOME/.profile/.alias

#################################
# ファンクション呼び出し
#################################
source $HOME/.profile/.functions

#################################
# .zsh.d呼び出し
#################################
read-dir `pwd`/.zsh.d/*.zsh

#################################
# 開発系呼び出し
#################################
read-dir `pwd`/.brew.d/*.zsh
