#################################
# Zshエイリアス設定
#################################
alias ll='ls -la'
alias zshconfig="atom ~/.zshrc"

#################################
# Selenium-Server用
#################################
#export firefox=""
alias selenium-server="selenium-server -browser browserName=firefox,version=30,platform=MAC,maxInstances=2 -Dwebdriver.firefox.bin=$HOME/Applications/Firefox.app/Contents/MacOS/firefox-bin"

#################################
# swift-cli用
#################################
alias swift-cli='/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/swift'

#################################
# Rsync用
#################################
alias rsync='rsync --no-p --exclude-from=$HOME/.rsync/exclude'

#################################
# PHP開発用
#################################
alias phpcs='phpcs --standard=PSR2 --colors --report-width=auto'
alias phpcbf='phpcbf --standard=PSR2'
alias php-cs-fixer='php-cs-fixer --level=psr2 --fixers=-psr0,-indentation,-braces fix'
alias ctags=/usr/local/Cellar/ctags/5.8_1/bin/ctags
alias artisan="$(which php) artisan"
alias phpunit="$(which phpunit) --no-globals-backup"

#################################
# Mecab用
#################################
alias mecab='mecab -d `mecab-config --dicdir`/mecab-ipadic-neologd'
